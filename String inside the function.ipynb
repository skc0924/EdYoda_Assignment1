   "source": [
    "def reverse(string):\n",
    "    \n",
    "    for i in string:\n",
    "        i=string[-1: :-1]\n",
    "    return i\n",
    "\n",
    "n=input(\"Enter the string: \")\n",
    "print(reverse(n))"
   ]
